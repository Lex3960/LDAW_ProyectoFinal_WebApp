<style type="text/css">
  .md-table-header {
    display: none;
  }
  .clearFilters {
    background-color: #EF5350;
  }
  .pagination li.active {
    background-color: #1976d2;
  }
  .ember-text-field{
    color: black;
  }
  .table-header:hover {
    cursor: pointer;
  }
  .table-header-no-sorting:hover {
    cursor: auto;
  }
  ::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
    color: #bdbdbd ;
    opacity: 1; /* Firefox */
  }
  .table-div {
    height: 57vh;
    overflow-y: scroll;
  }
</style>
<nav class="red lighten-1">
  <div class="row">
    <div class="col s12">
      <div class="nav-wrapper">
        <div class="col s12">
