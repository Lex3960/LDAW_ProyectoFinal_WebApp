<style type="text/css">
  .md-table-header {
    display: none;
  }
  .clearFilters {
    background-color: #c62828;
  }
  .pagination li.active {
    background-color: #01579b;
  }
  .ember-text-field{
    color: black;
  }
  .table-header:hover {
    cursor: pointer;
  }
  .table-header-no-sorting:hover {
    cursor: auto;
  }
  .table-container {
    height: 10em;
  }
  table {
      display: flex;
      flex-flow: column;
      height: 52vh;
      width: 100%;
  }
  table thead {
      flex: 0 0 auto;
      width: calc(100% - 0.9em);
  }
  table tbody {
      /* body takes all the remaining available space */
      flex: 1 1 auto;
      display: block;
      overflow-y: scroll;
      overflow-x: hidden;
  }
  table tbody tr {
      width: 100%;
  }
  table thead, table tbody tr {
      display: table;
      table-layout: fixed;
  }
  ::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
    color: #bdbdbd ;
    opacity: 1; /* Firefox */
  }
</style>
<nav class="red darken-3">
  <div class="row">
    <div class="col s12">
      <div class="nav-wrapper">
        <div class="col s12">
         {{#link-to 'auth.dashboard' class="breadcrumb"}}My English Academy {{/link-to}}
         {{#link-to 'auth.subscribers' class="breadcrumb"}}Subscriptores {{/link-to}}
       </div>
       </div>
    </div>
  </div>
</nav>

<div class="row">
  <div class="col s10 offset-s1">
    <blockquote> <h4> Subscriptores </h4> </blockquote>
  </div>
</div>

<div class="row ">
  <div class="container table-container col s10 offset-s1">
      {{models-table
        data=model
        columns=columns
        showColumnsDropdown=false
        customMessages=customMessages
        filteringIgnoreCase=true
        useNumericPagination=true
        pageSize=4
        delete='deleteSubscriber'
        edit='editSubscriber'
      }}
  </div>
</div>
